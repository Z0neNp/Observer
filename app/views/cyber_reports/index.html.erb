<div class="container">
  <div class="row">
    <h1 class="title-page">
      <%= page_title_for_cyber_reports_view("FriendlyResource \"#{friendly_resource_name(@friendly_resource)}\"") %>
    </h1>
  </div>
  <div class="row about-page">
    <p>
      You can navigate to any relevant
      <span data-toogle="tooltip" data-placement="top" title="A set of specific calculations / observations that lead to an assessment of whether friendly resource is under online attack.">
        <em>cyber report</em>
      </span>
      from here.
    </p>
  </div>
  <div class="row navigate-resources">
    <ul class="list-group">
      <% @cyber_report_types.each do |t| %>
        <li class="list-group-item">
          <%= link_to t, "show_reports/#{t}" %>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="row">
    <%= link_to "Back to \"#{friendly_resource_name(@friendly_resource)}\"", friendly_resource_path(@friendly_resource) %>
  </div>
</div>
